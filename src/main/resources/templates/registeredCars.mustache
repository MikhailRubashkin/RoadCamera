<html>

<body>
<div>
    <form method="post" action="/registeredCars">
        <input type="text" name="carNumber" placeholder="Add Car Number" />
        <button type="submit">Registration</button>
    </form>
</div>
<div>List Car Number</div>
<form method="post" action="/filter">
    <input type="text" name="filter">
    <button type="submit">Search</button>
</form>
<div>RegisteredCarsCount</div>
<form method="get" action="/registeredCars/count">
    <button type="submit">Count</button>
</form>
{{#carId}}
    <div>
        <b>RegisteredCarsCount: {{id}}</b>
    </div>
{{/carId}}

{{#cars}}
    <div>
        <b>{{id}}</b>
        <span>{{carNumber}}</span>
        <i>{{timestamp}}</i>
    </div>
{{/cars}}
</body>
</html>